[general]
state_file = <%= @state_file %>

<% @logs.each do |log| -%>
<% log.each do |name,path| -%>
[<%= name %>]
<% @datetime_formats.each do |logname,logdateformat| -%>
<% if logname = name%>
datetime_format = <%= logdateformat %>
<% end -%>
<% end -%>
file = <%= path %>
buffer_duration = 5000
log_stream_name = <%= @streamname %>
initial_position = start_of_file
log_group_name = <%= name %>
<% end -%>
<% end -%>
