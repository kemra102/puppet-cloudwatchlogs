[general]
state_file = <%= @state_file %>

<% @logs.each do |log| -%>
<% log.each do |name,path| -%>
[<%= name %>]
<%- logdatekey = false -%>
<%- @datetime_formats.each do |logformat| -%>
<%- if logformat.has_key?(name) -%>
<%- logdatekey = true -%>
datetime_format = <%= logformat[name] %>
<%- end -%>
<%- end -%>
<%- if !logdatekey -%>
datetime_format = %b %d %H:%M:%S
<%- end -%>
file = <%= path %>
buffer_duration = 5000
log_stream_name = <%= @streamname %>
initial_position = start_of_file
log_group_name = <%= name %>
<% end -%>
<% end -%>
